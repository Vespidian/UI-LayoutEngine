{
	classes: {
		"base": {
			padding: 6px,
			bottom: 10px,
			right: 10px,
			width: 250px,
			height: 50px,
			align: horizontal,
			color: #bb4d00,
		},
		"hover": {
			color: #62727b,
		}
		"color_base": {
			border: 0px,
			height: 100%,
			width: 25%,
			onhover: {
				class: "hover",
			}
		},
		"red":{
			color: #212121,
		}
		"green":{
			color: #484848,
		}
		"blue":{
			color: #f57c00,
		}
		"yellow":{
			color: #ffad42,
		}
		
	},
	body: {
		"container": {
			class: "base",
			elements: [
				"c1": {
					class: "color_base",
					class: "red",
					text: "1",
				},
				"c2": {
					class: "color_base",
					class: "green",
					text: "2",
				},
				"c3": {
					class: "color_base",
					class: "blue",
					text: "3",
				},
				"c4": {
					class: "color_base",
					class: "yellow",
					text: "4",
				},
			]
		}
	},
},